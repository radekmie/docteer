{
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": true,
        "regenerator": false,
        "useESModules": true
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": ["@babel/plugin-transform-regenerator"],
        "shippedProposals": true
      }
    ],
    ["@babel/preset-react", { "pragma": "h", "pragmaFrag": "Fragment" }],
    ["@babel/preset-typescript", { "jsxPragma": "h" }]
  ]
}
