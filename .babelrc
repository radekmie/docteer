{
  "env": {
    "production": {
      "plugins": [
        "./.babel-plugin.js"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-modules-commonjs",
    [
      "module-resolver",
      {
        "alias": {
          "@client": "./client",
          "@server": "./server",
          "@shared": "./shared",
          "@tests": "./tests",
          "@types": "./types"
        },
        "cwd": "babelrc"
      }
    ]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-flow",
    "@babel/preset-react"
  ]
}
