{
  "name": "docteer",
  "author": "Rados≈Çaw Miernik <radekmie@gmail.com>",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy": "cd .deploy && mup deploy",
    "deploy:clean": "cd .deploy && echo y | mup disk clean && mup disk show",
    "deploy:setup": "cd .deploy && mup setup",
    "e2e": "meteor test --full-app --driver-package universe:e2e --raw-logs --settings .deploy/settings-test.json",
    "flow": "flow",
    "lint": "eslint .",
    "precommit": "npm run test",
    "start": "meteor run --settings .deploy/settings-dev.json",
    "test": "npm run lint && npm run flow && CI=1 npm run e2e -- --once"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.40",
    "baobab": "2.5.1",
    "bcrypt": "1.0.3",
    "cssnano": "4.0.0-rc.2",
    "cssnano-preset-advanced": "4.0.0-rc.2",
    "etag": "1.8.1",
    "fuzzysort": "1.0.5",
    "history": "4.7.2",
    "html-minifier": "3.5.9",
    "meteor-node-stubs": "0.3.2",
    "postcss": "6.0.19",
    "postcss-cssnext": "3.1.0",
    "preact": "8.2.7",
    "preact-render-to-string": "3.7.0",
    "prepack": "0.2.23",
    "shrink-ray-current": "2.1.0",
    "simpl-schema": "1.4.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.18.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-vazco": "3.1.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-vazco": "1.0.0",
    "faker": "4.1.0",
    "flow-bin": "0.66.0",
    "flow-typed": "2.3.0",
    "husky": "0.14.3",
    "mocha": "4.1.0",
    "mup": "1.4.2",
    "mup-disk": "0.2.0",
    "prettier": "1.11.0",
    "puppeteer": "1.1.1"
  }
