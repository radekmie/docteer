{
  "name": "docteer",
  "author": "Rados≈Çaw Miernik <radekmie@gmail.com>",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy": "cd .deploy && mup deploy",
    "deploy:clean": "cd .deploy && echo y | mup disk clean && mup disk show",
    "deploy:setup": "cd .deploy && mup setup",
    "e2e": "rm -rf .coverage && COVERAGE=1 COVERAGE_APP_FOLDER=\"`pwd`/\" meteor test --full-app --driver-package universe:e2e --raw-logs --settings .deploy/settings-test.json",
    "e2e:ci": "CI=1 npm run e2e:once",
    "e2e:once": "npm run e2e -- --once",
    "flow": "flow",
    "lint": "eslint .",
    "precommit": "npm run test",
    "start": "meteor run --settings .deploy/settings-dev.json",
    "test": "npm run lint && npm run flow && npm run e2e:ci"
  },
  "meteor": {
    "mainModule": {
      "client": "client/index.js",
      "server": "server/index.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-rc.1",
    "baobab": "2.5.2",
    "bcrypt": "3.0.0",
    "cssnano": "4.0.5",
    "cssnano-preset-advanced": "4.0.0",
    "etag": "1.8.1",
    "fuzzysort": "1.1.4",
    "history": "4.7.2",
    "html-minifier": "3.5.20",
    "js-sha256": "0.9.0",
    "meteor-node-stubs": "0.4.1",
    "postcss": "7.0.2",
    "postcss-cssnext": "3.1.0",
    "preact": "8.3.1",
    "preact-render-to-string": "4.1.0",
    "prepack": "0.2.48",
    "shrink-ray-current": "2.1.2",
    "simpl-schema": "1.5.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "5.4.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-config-vazco": "4.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-vazco": "1.0.0",
    "faker": "4.1.0",
    "flow-bin": "0.79.1",
    "flow-typed": "2.5.1",
    "husky": "0.14.3",
    "mocha": "4.1.0",
    "mup": "1.4.5",
    "mup-disk": "0.2.0",
    "prettier": "1.14.2",
    "puppeteer": "1.7.0"
  }
}
