{
  "name": "docteer",
  "author": "Rados≈Çaw Miernik <radekmie@gmail.com>",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy": "cd .deploy && mup deploy",
    "deploy:clean": "cd .deploy && echo y | mup disk clean && mup disk show",
    "deploy:setup": "cd .deploy && mup setup",
    "e2e": "rm -rf .coverage && COVERAGE=1 COVERAGE_APP_FOLDER=\"`pwd`/\" meteor test --full-app --driver-package universe:e2e --raw-logs --settings .deploy/settings-test.json",
    "e2e:ci": "CI=1 npm run e2e:once",
    "e2e:once": "npm run e2e -- --once",
    "flow": "flow",
    "lint": "eslint .",
    "precommit": "npm run test",
    "start": "meteor run --settings .deploy/settings-dev.json",
    "test": "npm run lint && npm run flow && npm run e2e:ci"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.49",
    "baobab": "2.5.2",
    "bcrypt": "2.0.1",
    "cssnano": "4.0.0-rc.2",
    "cssnano-preset-advanced": "4.0.0-rc.2",
    "etag": "1.8.1",
    "fuzzysort": "1.1.2",
    "history": "4.7.2",
    "html-minifier": "3.5.16",
    "meteor-node-stubs": "0.4.1",
    "postcss": "6.0.22",
    "postcss-cssnext": "3.1.0",
    "preact": "8.2.9",
    "preact-render-to-string": "3.7.0",
    "prepack": "0.2.35",
    "shrink-ray-current": "2.1.0",
    "simpl-schema": "1.5.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-vazco": "3.1.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-vazco": "1.0.0",
    "faker": "4.1.0",
    "flow-bin": "0.73.0",
    "flow-typed": "2.4.0",
    "husky": "0.14.3",
    "mocha": "4.1.0",
    "mup": "1.4.4",
    "mup-disk": "0.2.0",
    "prettier": "1.13.4",
    "puppeteer": "1.4.0"
  }
}
